{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "warn",
        "no-param-reassign": "off",
        "quotes": [
            "error",
            "single"
        ],
        "linebreak-style": 0,
        "no-use-before-define": ["error", {"functions": false, "classes": false}]
    }
}
